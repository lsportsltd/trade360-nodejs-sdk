{
  "name": "trade360-nodejs-sdk",
  "version": "1.0.5",
  "description": "LSports Trade360 SDK for Node.js",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "rm -rf dist/ && tsc -p .",
    "build:dev": "npm run lint && npm run build",
    "watch": "tsc -w",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "lsports",
    "trade360",
    "sdk",
    "sports",
    "betting",
    "api"
  ],
  "author": "LSports",
  "license": "ISC",
  "dependencies": {
    "amqplib": "^0.10.4",
    "axios": "^1.7.4",
    "body-parser": "^1.20.2",
    "bunyan": "^1.8.15",
    "class-transformer": "^0.5.1",
    "express": "^4.19.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "pino": "^9.4.0",
    "reflect-metadata": "^0.2.2",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.5",
    "@types/axios": "^0.14.0",
    "@types/bunyan": "^1.8.11",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2"
  },
  "_moduleAliases": {
    "@feed": "dist/src/feed",
    "@feed/*": "dist/src/feed/*",
    "@entities": "dist/src/entities",
    "@lsports/entities": "dist/src/entities/core-entities",
    "@lsports/entities/*": "dist/src/entities/core-entities/*",
    "@lsports/enums": "dist/src/entities/core-entities/enums",
    "@lsports/decorators": "dist/src/entities/decorators",
    "@lsports/errors": "dist/src/entities/errors",
    "@api": "dist/src/api",
    "@api/*": "dist/src/api/*",
    "@customers-api": "dist/src/api/customers-api",
    "@customers-api/*": "dist/src/api/customers-api/*",
    "@metadata-api": "dist/src/api/common/metadata",
    "@metadata-api/*": "dist/src/api/common/metadata/*",
    "@subscription-api": "dist/src/api/common/subscription",
    "@subscription-api/*": "dist/src/api/common/subscription/*",
    "@httpClient": "dist/src/api/base-http-client",
    "@httpClient/*": "dist/src/api/base-http-client/*",
    "@utilities": "dist/src/utilities",
    "@logger": "dist/src/logger",
    "@api/common/snapshot": "dist/src/api/common/snapshot",
    "@api/snapshot-api/snapshot-api": "dist/src/api/snapshot-api/snapshot-api",
    "@api/snapshot-api/interfaces": "dist/src/api/snapshot-api/interfaces"
  }
}